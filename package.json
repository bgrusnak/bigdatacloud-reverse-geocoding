{
  "name": "bigdatacloud-reverse-geocoding",
  "version": "0.1.0",
  "description": "Reverse geocoding browser-based typescript library, using free Bigdatacloud API",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgrusnak/bigdatacloud-reverse-geocoding.git"
  },
  "keywords": [
    "typescript",
    "bigdatacloud",
    "reverse",
    "geocoding",
    "browser"
  ],
  "author": {
    "name": "Ilya A. Shlyakhovoy",
    "email": "bulgarus@inbox.ru"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bgrusnak/bigdatacloud-reverse-geocoding/issues"
  },
  "homepage": "https://github.com/bgrusnak/bigdatacloud-reverse-geocoding#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
